<script>
// Cargar carrito
let carrito = JSON.parse(localStorage.getItem("carrito")) || [];

// Guardar en localStorage
function guardarCarrito() {
  localStorage.setItem("carrito", JSON.stringify(carrito));
}

// AGREGAR PRODUCTO
function agregarProducto(nombre, precio, img) {
  carrito.push({ nombre, precio, img });
  guardarCarrito();
  alert("Producto agregado al carrito ✔️");
}

// Conectar botones
document.addEventListener("DOMContentLoaded", () => {
  const productos = document.querySelectorAll(".item"); // tus tarjetas reales

  productos.forEach(prod => {
    const btn = prod.querySelector("button");

    btn.addEventListener("click", () => {
      const nombre = prod.querySelector("h3").innerText;
      const precio = parseInt(prod.querySelector("p").innerText.replace("$",""));
      const img = prod.querySelector("img").src;

      agregarProducto(nombre, precio, img);
    });
  });
});
</script>

